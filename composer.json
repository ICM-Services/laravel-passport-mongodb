{
    "name": "icm-services/laravel-passport-mongodb",
    "description": "Laravel Passport provides OAuth2 server support to Laravel.",
    "keywords": ["laravel", "passport", "oauth"],
    "license": "MIT",
    "authors": [
        {
            "name": "ICM-Services",
            "email": "technic@icm-services.fr"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "firebase/php-jwt": "~3.0|~4.0|~5.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/auth": "8.*",
        "illuminate/console": "8.*",
        "illuminate/container": "8.*",
        "illuminate/contracts": "8.*",
        "illuminate/database": "8.*",
        "illuminate/encryption": "8.*",
        "illuminate/http": "8.*",
        "illuminate/support": "8.*",
        "league/oauth2-server": "^7.0",
        "phpseclib/phpseclib": "^2.0",
        "symfony/psr-http-message-bridge": "~1.0",
        "zendframework/zend-diactoros": "~1.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "MoeenBasra\\LaravelPassportMongoDB\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "MoeenBasra\\LaravelPassportMongoDB\\PassportServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
