{
    "name": "icm-services/laravel-passport-mongodb",
    "description": "Laravel Passport provides OAuth2 server support to Laravel.",
    "keywords": ["laravel", "passport", "oauth"],
    "license": "MIT",
    "authors": [
        {
            "name": "ICM-Services",
            "email": "technic@icm-services.fr"
        }
    ],
    "require": {
        "php": ">=8.0.1",
        "firebase/php-jwt": "6.2.0",
        "guzzlehttp/guzzle": "^7.4.5",
        "illuminate/auth": "12.*",
        "illuminate/console": "12.*",
        "illuminate/container": "12.*",
        "illuminate/contracts": "12.*",
        "illuminate/database": "12.*",
        "illuminate/encryption": "12.*",
        "illuminate/http": "12.*",
        "illuminate/support": "12.*",
        "lcobucci/jwt": "^5.0",
        "league/oauth2-server": "^9.0",
        "phpseclib/phpseclib": "^2.0",
        "symfony/psr-http-message-bridge": "^7.1.4",
        "laminas/laminas-diactoros": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "MoeenBasra\\LaravelPassportMongoDB\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "MoeenBasra\\LaravelPassportMongoDB\\PassportServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
